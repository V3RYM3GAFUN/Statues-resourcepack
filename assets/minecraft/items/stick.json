{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/stick"
        },
        "cases": [
            {
                "when": "stun_baton",
                "model": {
                    "type": "model",
                    "model": "statues/weapons/stun_baton"
                }
            },
            {
                "when": "elephant",
                "model": {
                    "type": "model",
                    "model": "statues/elephant"
                }
            },
            {
                "when": "elevator_new",
                "model": {
                    "type": "model",
                    "model": "statues/machines/elevator_new"
                }
            },
            {
                "when": "small_elevator",
                "model": {
                    "type": "model",
                    "model": "statues/machines/small_elevator"
                }
            },
            {
                "when": "freight_elevator",
                "model": {
                    "type": "model",
                    "model": "statues/machines/freight_elevator"
                }
            },
            {
                "when": "crowbar",
                "model": {
                    "type": "model",
                    "model": "statues/items/crowbar"
                }
            },
            {
                "when": "flashlight",
                "model": {
                    "type": "model",
                    "model": "statues/items/flashlight"
                }
            },
            {
                "when": "3x3_door_left",
                "model": {
                    "type": "model",
                    "model": "statues/machines/3x3_door_left"
                }
            },
            {
                "when": "3x3_door_right",
                "model": {
                    "type": "model",
                    "model": "statues/machines/3x3_door_left"
                }
            },
            {
                "when": "java_chan",
                "model": {
                    "type": "model",
                    "model": "statues/java_chan"
                }
            }
        ]
    }
}